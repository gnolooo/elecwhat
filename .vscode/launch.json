{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "elecwhat",
      "program": "${workspaceFolder}",
      "request": "launch",
      "runtimeExecutable": "electron",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "elecwhat debug",
      "program": "${workspaceFolder}",
      "request": "launch",
      "runtimeExecutable": "electron",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "env": {
        "DEBUG": "1"
      }
    }
  ]
}
